//“就地"逆置链表，辅助空间复杂度为O(1)
//用头插法查到尾结点
//逆置之后没有头结点,所以重建
void  trans(LinkList &L)
{
    LNode *p=L;//找到尾结点
    while(p->next!=NULL)
    {
        p=p->next;
    }
    L=L->next;
    //头插法依次插到尾结点后面
	LNode *tmp;//暂存后继结点，避免一次循环过后断链
    while(L!=p)
    {
       	tmp=L->next
        L->next=p->next;
        p->next=L;
        L=tmp;//恢复下一次循环要使用的结点
    }
    //重建头结点
    LNode *s=new LNode;
    s->next=L;
    L=s;
}
